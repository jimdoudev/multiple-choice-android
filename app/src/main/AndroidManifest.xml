<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.multiplechoiceproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/multiple_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/multiple_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MultipleChoiceProject">
        <activity
            android:name=".Evaluation"
            android:exported="false" />
        <activity
            android:name=".MultipleChoice"
            android:exported="false"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".DataEntry"
            android:exported="false"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".Menu"
            android:exported="false"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".WelcomePage"
            android:exported="true"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>